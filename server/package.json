{
  "name": "surf_api",
  "version": "1.0.0",
  "description": "typescript surf api",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "dev": "ts-node-dev src/index.ts | pino-pretty -c",
    "lint:check": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "repository": "https://github.com/eduardosdl/surf_api",
  "author": "eduardosdl <edu.silva.e08@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.11.19",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "nock": "^13.3.0",
    "pino-pretty": "^9.4.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^6.9.2",
    "pino": "^8.11.0"
  }
