{
  "name": "surf_api",
  "version": "1.0.0",
  "description": "typescript surf api",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node build dist/src/index.js",
    "dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest --projects ./test --runInBand",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "repository": "https://github.com/eduardosdl/surf_api",
  "author": "eduardosdl <edu.silva.e08@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.3.2",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "module-alias": "^2.2.2"
  }
}
